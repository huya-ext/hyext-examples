.container {
  justify-content: center;
  align-items: center;
  background-color: white;
  position: absolute;
  right: 0;
  left: 0;
  top: 0;
  bottom: 0;
}
.section {
  padding: 10px;
  width: 650px;
}
.title {
  font-weight: bold;
  margin-bottom: 5px;
}
.button {
  height: 85px;
  width: 600px;
  background-color: #ff9600;
  border-radius: 10px;
}

.image{
  width: 100px;
  height: 100px;
}
